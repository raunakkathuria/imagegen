# Model Selection (stable-diffusion, stable-diffusion-xl)
MODEL_TYPE=stable-diffusion-xl
MODEL_ID=stabilityai/stable-diffusion-xl-base-1.0
VARIANT=fp16

# Model Parameters
GUIDANCE_SCALE=7.5            # Range 7.0-12.0; Higher values = more prompt adherence
NUM_INFERENCE_STEPS=30        # Range 50-100; More steps = more detailed results
HEIGHT=768                    # Optimal size for SD
WIDTH=768                     # Optimal size for SD

USE_GPU=true
# Memory Optimization (SDXL requires more VRAM)
ENABLE_ATTENTION_SLICING=true
ENABLE_MEMORY_EFFICIENT_ATTENTION=true
ENABLE_VAE_TILING=true
ENABLE_VAE_SLICING=true
USE_SEQUENTIAL_CPU_OFFLOAD=true  # Use if you have limited VRAM

# Additional Parameters
SCHEDULER="LMSDiscreteScheduler"

# API Configuration
PORT=8080
HOST=0.0.0.0
